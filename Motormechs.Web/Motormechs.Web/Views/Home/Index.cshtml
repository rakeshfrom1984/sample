<link href="~/Scripts/Slider/style.css" rel="stylesheet" />
<script src="~/Scripts/Slider/responsiveslides.min.js"></script>
<script>
    // You can also use "$(window).load(function() {"
    jQuery(function () {
        // Slideshow 3
        jQuery("#slider3").responsiveSlides({
            manualControls: '#slider3-pager',
            //maxwidth: 100 + "%"
        });
    });
</script>
<section id="home">
    <div class="banner-container">
        
        <style>
            .slider {
                position: relative;
            }

            .rslides {
                position: relative;
                list-style: none;
                overflow: hidden;
                width: 100%;
                padding: 0;
                margin: 0;
                max-height: 545px;
                /*max-height: 600px;*/
            }

                .rslides li {
                    -webkit-backface-visibility: hidden;
                    position: absolute;
                    display: none;
                    width: 100%;
                    left: 0;
                    top: 0;
                    max-height: 600px;
                }

                    .rslides li:first-child {
                        position: relative;
                        display: block;
                        float: left;
                    }

                .rslides img {
                    display: block;
                    height: auto;
                    float: left;
                    width: 100%;
                    border: 0;
                }

            .slider a, .slider a:hover {
                display: block;
                text-decoration: none;
            }

            .sliderContent {
                display: block;
            }

            .divMain {
                background-color: #fff !important;
            }

            .InnerContent {
                width: 100%;
            }
        </style>

        <div class="slider">
            <ul class="rslides" id="slider3">
                <li>
                    <img src="/Scripts/Slider/a0.jpg" alt="">
                    <div class="sliderContent">
                        <div class="description">
                            <h2>Multi Brand Car Service</h2>
                        </div>
                        <div class="clearfix row-fluid"></div>
                        <div class="description marginD">
                            <h2>Car Servicing</h2>
                        </div>

                    </div>
                </li>
                <li>
                    <img src="/Scripts/Slider/a1.jpg" alt="">
                    <div class="sliderContent">
                        <div class="description">
                            <h2>Multi Brand Car Service</h2>
                        </div>
                        <div class="clearfix row-fluid"></div>
                        <div class="description marginD">
                            <h2>Car Foam Wash</h2>
                        </div>

                    </div>
                </li>
                <li>
                    <img src="/Scripts/Slider/a2.jpg" alt="">
                    <div class="sliderContent">
                        <div class="description">
                            <h2>Multi Brand Car Service</h2>
                        </div>
                        <div class="clearfix row-fluid"></div>
                        <div class="description marginD">
                            <h2>Compurerised Car Diagnosis</h2>
                        </div>

                    </div>
                </li>
            </ul>
            <div class="description2 feature">
                <div class="innerFeature" style="">
                    <a class="btn" href="/home/new/service">Car Servicing</a>
                    <a class="btn" href="/home/new/spa">Car Spa</a>
                    </div>
                    @*<a class="btn" href="#">Denting and Painting</a>*@
                </div>
        </div>

        <!-- <a class="hero-button learn-more smoothscroll text-center" href="#features">Learn More</a>-->
        <!-- <div class="hero-img"> <img src="images/homepage-1204-background-lapto.png" alt="" class="text-center animated fadeInUpBig"/></div>-->
        @*</div>*@
    </div>
    <div class="container hero-text2" style="text-align:center;">
        <h1 style="color: #b42a28;">Multi Brand Car Service Station</h1>
        <img class="MultiBrandImg" src="~/Content/images/MultiBrand.png" />
    </div>
</section>

